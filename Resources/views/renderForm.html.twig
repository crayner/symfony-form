<!-- Form React Script -->
<script language="JavaScript" type="text/javascript">
    window.FORM_PROPS = {{ props|json_encode|raw }}
    $(document).ready(function(){
        {% set link = encore_entry_css_files('form') %}
        {% dump(link) %}
        if (document.createStyleSheet){  // Bloody IE
            {% for css in link %}
                document.createStyleSheet('{{ asset(css) }}');
            {% endfor %}
        }
        else {
            {% for css in link %}
                $("head").append($("<link rel='stylesheet' href='{{ asset(css) }}' type='text/css' media='screen' />"));
            {% endfor %}
        }
    });
</script>
{{ encore_entry_script_tags('form') }}
